{
  "name": "@furiouzz/lol",
  "version": "0.0.36-beta.4",
  "type": "module",
  "main": "./esm/index.js",
  "types": "./types/index.d.ts",
  "typings": "./types",
  "description": "LOL JS Utils",
  "scripts": {
    "build": "node build.mjs",
    "bump": "node bump.mjs",
    "publish": "node publish.mjs"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "author": "Christophe Massolin <frenchise93@gmail.com> (https://chrsmsln.com)",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/FuriouZz/lol.js"
  },
  "dependencies": {
    "filelist": "github:FuriouZz/filelist",
    "semver": "^7.3.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "typescript": "^4.6.3"
  },
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js",
      "types": "./types/index.d.ts"
    },
    "./*": {
      "require": "./cjs/*",
      "import": "./esm/*",
      "types": "./types/*"
    },
    "./array": {
      "require": "./cjs/array/index.js",
      "import": "./esm/array/index.js",
      "types": "./types/array/index.d.ts"
    },
    "./collection": {
      "require": "./cjs/collection/index.js",
      "import": "./esm/collection/index.js",
      "types": "./types/collection/index.d.ts"
    },
    "./dom": {
      "require": "./cjs/dom/index.js",
      "import": "./esm/dom/index.js",
      "types": "./types/dom/index.d.ts"
    },
    "./function": {
      "require": "./cjs/function/index.js",
      "import": "./esm/function/index.js",
      "types": "./types/function/index.d.ts"
    },
    "./math": {
      "require": "./cjs/math/index.js",
      "import": "./esm/math/index.js",
      "types": "./types/math/index.d.ts"
    },
    "./node": {
      "require": "./cjs/node/index.js",
      "import": "./esm/node/index.js",
      "types": "./types/node/index.d.ts"
    },
    "./object": {
      "require": "./cjs/object/index.js",
      "import": "./esm/object/index.js",
      "types": "./types/object/index.d.ts"
    },
    "./promise": {
      "require": "./cjs/promise/index.js",
      "import": "./esm/promise/index.js",
      "types": "./types/promise/index.d.ts"
    },
    "./string": {
      "require": "./cjs/string/index.js",
      "import": "./esm/string/index.js",
      "types": "./types/string/index.d.ts"
    }
  }
}